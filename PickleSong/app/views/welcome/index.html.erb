<div class="section-outter">
    <div class="section-wrapper">
        <% if @results %>
        <div class="col-md-9">
            <h1><%= @first_result.title %></h1>
            <iframe width="853" height="480" src="//<%= @first_result.embed_url %>" frameborder="0" allowfullscreen></iframe>
       
            <div id="results">
                <div class="section-inner">
                    <ul id="og-grid" class="og-grid">
                        <% @alternatives.each do |result| %>
                        <li>
                            <div class="og-overlay"></div>
                            <p class="og-title truncate"><%= result.title %></p>
                            <iframe width="372" height="210" src="//<%= result.embed_url %>" frameborder="0" allowfullscreen></iframe>
                        </li>
                        <% end %>
                    </ul>
                </div>
            </div><!--results-->
         </div><!--col-md-9-->
        <% end %>
        <div class="col-md-3">
            <%= render "form" %>
        </div>
    </div><!--section-wrapper-->
  <%= render "playlist_panel"%>
</div><!--section-outer-->